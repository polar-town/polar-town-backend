{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-param-reassign": "off",
    "import/prefer-default-export": "off",
    "no-promise-executor-return": "off",
    "import/order": "off",
    "no-use-before-define": "off",
    "no-unused-vars": "warn"
  }
}
